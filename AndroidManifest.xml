<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.demorestclient"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="16" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.demorestclient.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.demorestclient.ListSongs"
            android:label="@string/title_activity_list_songs" >
        </activity>
        <activity
            android:name="com.example.demorestclient.SearchSong"
            android:label="@string/title_activity_search_song" >
        </activity>
        <activity
            android:name="com.example.demorestclient.CreateSong"
            android:label="@string/title_activity_create_song" >
        </activity>
    </application>

</manifest>